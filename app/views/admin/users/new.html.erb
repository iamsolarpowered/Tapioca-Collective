<h1>New user</h1>

<% form_for(:admin, @user) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => 'form', :locals => {:f => f} %>

  <%= render :partial => 'password', :locals => {:f => f} %>

  <p>
    <%= f.submit 'Create', :class => 'submit' %>
    <%= link_to 'Cancel', admin_users_path %>
  </p>
<% end %>
